{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "pretty": true,
    "strictNullChecks": false,
    "target": "es5"
  },
  "include": [
    "./src/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
